# Example environment configuration for Project Lightspeed
# Rename this file to `.env` and adjust the values as needed.

# Hostname/IP of the WebRTC server accessible by the ingest and clients
WEBSOCKET_HOST=localhost

# Ports exposed on the host machine
INGEST_PORT=8084
WEB_PORT=8000
WEBSOCKET_PORT=8080

# Additional configuration
ICE_SERVERS=stun:stun.l.google.com:19302
WEBRTC_SERVER_URL=http://localhost:8080
AUTH_WS_URL=ws://localhost:8082
